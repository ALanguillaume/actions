{
    "problemMatcher": [
        {
            "owner": "rcmdcheck",
            "pattern": [
                {
                    "regexp": "^(.*) [.][.][.] (ERROR|NOTE|WARNING)$",
                    "code": 1,
                    "severity": 2
                },
                {
                    "regexp": "^([^*].+)$",
                    "message": 1,
                    loop: true
                }
            ]
        }
    ]
}
